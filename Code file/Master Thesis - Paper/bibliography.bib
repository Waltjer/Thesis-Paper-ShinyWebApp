@article{jomouiGeneticOriginA0thalassemia2017,
  title = {Genetic Origin of {$A$}0-Thalassemia ({{SEA}} Deletion) in {{Southeast Asian}} Populations and Application to Accurate Prenatal Diagnosis of {{Hb Bart}}'s Hydrops Fetalis Syndrome},
  author = {Jomoui, Wittaya and Fucharoen, Goonnapa and Sanchaisuriya, Kanokwan and Charoenwijitkul, Patnaree and Maneesarn, Jitpanu and Xu, Xiangmin and Fucharoen, Supan},
  year = {2017},
  month = aug,
  journal = {Journal of Human Genetics},
  volume = {62},
  number = {8},
  pages = {747--754},
  issn = {1435-232X},
  doi = {10.1038/jhg.2017.41},
  abstract = {{$\alpha$}0-thalassemia of SEA deletion (-SEA) is common among Southeast Asian and Chinese. Using haplotype and phylogenetic analyses, we examined the origin of this defect in Southeast Asian populations. Study was done on both normal and {$\alpha$}0-thalassemia alleles in 3 ethnic groups including 96 Thai, 52 Laotian and 21 Cambodian. Five SNPs encompassing the (-SEA) including (rs3760053 T{$>$}G), (rs1211375 A{$>$}C), (rs3918352 A{$>$}G), (rs1203974 A{$>$}G) and (rs11248914 C{$>$}T) were examined using high-resolution melting assays. It was found that 94.0\% of Thai, 100\% of Laotian and 100\% of Cambodian {$\alpha$}0-thalassemia alleles were linked to the same haplotype: the haplotype H4 (AAGC), representing an Asian specific origin. An G allele of the (rs3760053) was found to be in strong linkage disequilibrium with the {$\alpha$}0-thalassemia allele in these populations. A multiplex PCR assay was developed to detect simultaneously the (-SEA) allele and genotyping of a linked (rs3760053) to improve accuracy of prenatal diagnosis of {$\alpha$}0-thalassemia. Application of this multiplex PCR assay for routine prenatal diagnosis of {$\alpha$}0-thalassemia in 12 families revealed a 100\% concordant result with conventional gap-PCR assay. Therefore, a single genetic origin is responsible for the spread and high prevalence of the (-SEA) in the region. The multiplex PCR assay developed should provide a double-check PCR system for more accurate diagnosis and allow the monitoring of possible maternal contamination at prenatal diagnosis of this important genetic disorder.},
  langid = {english},
  pmcid = {PMC5584512},
  pmid = {28381876},
  keywords = {alpha-Thalassemia,Asia Southeastern,Female,Genetic Carrier Screening,Genotype,Hemoglobins Abnormal,Humans,Hydrops Fetalis,Multiplex Polymerase Chain Reaction,Phylogeny,Pregnancy,Prenatal Diagnosis,Sequence Deletion},
