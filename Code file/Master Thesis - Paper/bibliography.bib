@article{spencerInfluenceEthnicOrigin2000,
  title = {The Influence of Ethnic Origin on First Trimester Biochemical Markers of Chromosomal Abnormalities},
  author = {Spencer, Kevin and Ong, Charas Y. T. and Liao, Adolfo W. J. and Nicolaides, Kypros H.},
  year = {2000},
