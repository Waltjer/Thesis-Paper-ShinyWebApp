@article{burkeGenderIdentitySex1989,
  title = {Gender {{Identity}}, {{Sex}}, and {{School Performance}}},
  author = {Burke, Peter J.},
  year = {1989},
  journal = {Social Psychology Quarterly},
  volume = {52},
  number = {2},
  eprint = {2786915},
  eprinttype = {jstor},
  pages = {159--169},
  publisher = {[Sage Publications, Inc., American Sociological Association]},
  issn = {0190-2725},
  doi = {10.2307/2786915},
  urldate = {2021-11-25},
  abstract = {Although sex differences in grades received in school are known to exist, often they have been attributed, simply a priori, to sex role gender identity differences between boys and girls. What little empirical research exists on this question is inadequate and mixed in its results. The present research was designed to overcome these problems by using a large, broad-based sample, a theoretically based bipolar measure of gender role identity, and course grade point average (GPA) across a number of academic subjects. A study of 1,688 sixth, seventh, and eight graders shows that the degree of masculinity or femininity of their gender identities accounts for a significant portion of their school grades. The pattern that is found across all subject areas shows that the more feminine the gender identity of both boys and girls, the better their performance as measured by GPA in classes. This pattern remains constant across controls for sex, race, grade, and sex of teacher. Although gender identity has significant effects, there remain sizable sex differences in performance which are not related to gender identity but which, because of their patterning, seem to be social in origin.}
}

