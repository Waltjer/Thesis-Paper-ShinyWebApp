@article{careyPerspectivesCareManagement2012,
  title = {Perspectives on the Care and Management of Infants with Trisomy 18 and Trisomy 13: Striving for Balance},
  shorttitle = {Perspectives on the Care and Management of Infants with Trisomy 18 and Trisomy 13},
  author = {Carey, John C.},
